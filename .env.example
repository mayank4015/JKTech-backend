# Server Configuration
PORT=8080
FRONTEND_URL=http://localhost:3000

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# S3 Configuration for Supabase Storage Backend (Optional)
S3_BUCKET_NAME=your-s3-bucket-name
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=your_s3_access_key
S3_SECRET_ACCESS_KEY=your_s3_secret_key
S3_ENDPOINT=https://s3.amazonaws.com

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=1h
REFRESH_TOKEN_EXPIRES_IN=7d

# OAuth Configuration
GOOGLE_OAUTH_REDIRECT_URL=http://localhost:3000/auth/callback

# Rate Limiting Configuration
THROTTLE_TTL=60
THROTTLE_LIMIT=10

# Database Connection Configuration
DATABASE_URL="postgresql://postgres:your_password@localhost:5432/your_database?schema=public"
DIRECT_URL="postgresql://postgres:your_password@localhost:5432/your_database?schema=public"
DB_MAX_RETRIES=3
DB_RETRY_DELAY=1000
DB_MAX_RETRY_DELAY=10000
DB_CONNECTION_TIMEOUT=30000